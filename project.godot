; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Genshin-slime"
run/main_scene="res://Menu/MainMenu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://images/Icone.png"

[autoload]

State="*res://QuestsNDialogue/state.gd"
Signals="*res://scripts/Signals.gd"
Events="*res://scripts/Events.gd"
QuestSystem="*res://addons/quest_system/quest_manager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
Shortcuts="*res://QuestsNDialogue/shortcuts.gd"
InventoryInstance="*res://Inventory/inventory_instance.gd"
SceneManagment="*res://scripts/SceneManagment.gd"
SaveManager="*res://scripts/SaveManager.gd"

[dialogue_manager]

general/states=["State", "Shortcuts"]

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="viewport"

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/quest_system/plugin.cfg")

[file_customization]

folder_colors={
"res://MapTools/": "yellow",
"res://Menu/": "purple",
"res://Player/": "green",
"res://grap_resurce/": "pink",
"res://maps/": "red",
"res://music/": "pink",
"res://npc/": "purple",
"res://scripts/": "blue"
}

